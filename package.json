{
  "name": "tada",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "start": "next start",
    "hrd": "TS_NODE_PROJECT='./ethereum/tsconfig.json' TS_NODE_TRANSPILE_ONLY=1 npx hardhat",
    "clean:eth": "rimraf ./ethereum/cache ./ethereum/cache-ovm ./ethereum/artifacts ./ethereum/artifacts-ovm",
    "compile:eth": "yarn clean:eth && yarn hrd compile",
    "compile:ovm": "yarn clean:eth && yarn hrd --network optimism compile",
    "deploy:local:eth": "yarn hrd run ethereum/scripts/deploy.ts --network localhost",
    "deploy:local:ovm": "yarn hrd run ethereum/scripts/deploy-ovm.ts --network optimism",
    "deploy:prod:ovm": "yarn hrd run ethereum/scripts/deploy-ovm.ts --network optimism-prod",
    "test:eth": "yarn hrd test",
    "test:ovm": "yarn hrd --network optimism-prod test"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.5.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@eth-optimism/hardhat-ovm": "^0.1.0",
    "@openzeppelin/contracts": "3.4.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.sortby": "^4.7.6",
    "dotenv": "^8.2.0",
    "framer-motion": "^4",
    "js-cookie": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.sortby": "^4.7.0",
    "next": "10.1.3",
    "next-cookies": "^2.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-login": "^5.2.2",
    "react-icons": "^4.2.0",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@types/chai": "^4.2.16",
    "@types/js-cookie": "^2.2.6",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.2.0",
    "hardhat-deploy": "^0.7.5",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
